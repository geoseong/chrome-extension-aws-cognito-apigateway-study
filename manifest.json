{
  "name": "WIK test",
  "version": "1.3",
  "manifest_version": 2,

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "wik_19.png",
    "default_popup": "index.html",
    "browser_style": true
  },
  "icons": {
    "16": "wik_16.png",
    "19": "wik_19.png",
    "48": "wik_48.png",
    "128": "wik_128.png"
  },
  "permissions": [
    "notifications",
    "contextMenus",
    "background",
    "storage",
    "unlimitedStorage",
    "tabs",
    "<all_urls>",
    "identity",
    "*://www.facebook.com/*",
    "*://graph.facebook.com/*",
    "*://www.googleapis.com/*",
    "*://accounts.google.com/*"
  ]
}